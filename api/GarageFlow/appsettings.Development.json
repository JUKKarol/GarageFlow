{
  "AppSettings": {
    "ConnectionStrings": {
      "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=GarageFlow;Trusted_Connection=True;"
    },
    "Sieve": {
      "DefaultPageSize": 20,
      "MaxPageSize": 20
    },
    "EmailCreds": {
      "Address": "yourEmailAddress",
      "Password": "",
      "Port": "587",
      "Host": "smtp.ethereal.email"
    },
    "AllowedOrigins": [
      "http://localhost:8080",
      "http://localhost:3000"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] |{SourceContext}| {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}